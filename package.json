{
  "name": "vue3-cute-timeline",
  "version": "0.0.7",
  "private": false,
  "description": "Vue3 时间线组件 ",
  "module": "./dist/vue3-timeline.es.js",
  "main": "./dist/vue3-timeline.umd.js",
  "exports": {
    ".": {
      "import": "./dist/vue3-timeline.es.ts",
      "require": "./dist/vue3-timeline.umd.ts"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "keywords": [
    "timeline",
    "vue-timeline",
    "vue3-timeline",
    "vue3Timeline"
  ],
  "license": "MIT",
  "author": "xiaojieajie <https://github.com/xiaojieajie>",
  "scripts": {
    "dev": "vite --mode dev --open",
    "build": "vue-tsc --noEmit && vite build --mode prod",
    "lint": "eslint \"**/*.{vue,ts,js}\"",
    "test": "vitest",
    "lint:fix": "npm run lint -- --fix",
    "release": "bumpp package.json --commit --push --tag"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@black-tooth/eslint-config": "0.0.12",
    "@types/node": "17.0.18",
    "@vitejs/plugin-vue": "2.2.2",
    "@vueuse/core": "7.6.2",
    "bumpp": "^7.1.1",
    "eslint": "8.9.0",
    "typescript": "4.5.5",
    "unplugin-auto-import": "^0.6.0",
    "vite": "2.8.4",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vitest": "0.5.0",
    "vue": "^3.2.29",
    "vue-tsc": "0.31.4"
  }
}
